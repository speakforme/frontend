{
  "name": "speakforme",
  "version": "1.0.0",
  "description": "speakforme.in",
  "dependencies": {
    "autoprefixer": "7.1.5",
    "babel-core": "^6.26.3",
    "babel-eslint": "8.0.1",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-inferno": "^3.3.0",
    "babel-preset-inferno-app": "^6.0.0",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "2.1.0",
    "connect-history-api-fallback": "1.4.0",
    "cross-spawn": "5.1.0",
    "css-loader": "0.28.7",
    "detect-port": "1.2.1",
    "dotenv": "4.0.0",
    "eslint": "4.18.2",
    "eslint-config-inferno-app": "^5.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-inferno": "7.4.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "extract-text-webpack-plugin": "3.0.1",
    "file-loader": "1.1.5",
    "fs-extra": "4.0.2",
    "html-webpack-plugin": "5.5.0",
    "http-proxy-middleware": "^0.19.1",
    "inferno": "^3.10.1",
    "inferno-component": "^3.10.1",
    "inferno-dev-utils": "^4.0.0",
    "inferno-error-overlay": "^3.0.0",
    "jest": "21.2.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "^0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.7.1",
    "webpack-dev-server": "3.1.11",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "csv-loader": "^2.1.1",
    "gulp": "^3.9.1",
    "gulp-cli": "^1.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^3.0.0",
    "gulp-yaml": "^1.0.1",
    "json-loader": "^0.5.7",
    "yaml-frontmatter-loader": "^0.1.0",
    "yaml-loader": "^0.5.0",
    "yamljs": "^0.3.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/speakforme/frontend.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/speakforme/frontend/issues"
  },
  "homepage": "https://speakforme.in/",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react$": "inferno-compat",
      "^react-dom$": "inferno-compat"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "inferno-app"
    ]
  },
  "eslintConfig": {
    "extends": "inferno-app"
  }
}
